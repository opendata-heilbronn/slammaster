{
  "name": "svn",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/electron-main.js",
  "scripts": {
    "build": "ng build --base-href '' && npm run copy:dist",
    "start": "electron .",
    "start:dev": "cross-env PACKAGE=false HOST=http://localhost:8080 npm run start:dev-impl",
    "start:dev-impl": "concurrently \"ng build -w\" \"npm run serve\" \"await tcp localhost:8080 && npm run start\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "copy:dist": "node build/copy-dist.js",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "serve": "http-server dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^5.0.0",
